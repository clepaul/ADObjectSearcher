<Window x:Class="ADObjectSearcher.ADObjectViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADObjectSearcher"
        mc:Ignorable="d"
        Title="ADObjectViewWindow" Height="900" Width="1024" Loaded="Window_Loaded">
    <Grid>
        <DataGrid x:Name="dataGridADObjects" Margin="10" IsReadOnly="True" FontFamily="Consolas" FontSize="16" AlternatingRowBackground="#FFE8F2FF" />
    </Grid>
</Window>
